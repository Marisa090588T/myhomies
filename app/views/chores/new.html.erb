<h1>Create new chore</h1>

<div>
  <%= simple_form_for [@home, @chore] do |f|%>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.button :submit, value: 'Add chore', class: 'btn btn-primary' %>
  <% end %>
</div>

<div class="links">
    <%= link_to 'cancel', dashboard_path %>
</div>